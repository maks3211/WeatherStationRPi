<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="1024" d:DesignHeight="470"
             xmlns:vm="using:AvaloniaTest.ViewModels"
             xmlns:vm2 ="using:AvaloniaTest.Models.WeatherForecast"
             xmlns:local="clr-namespace:AvaloniaTest.Views"
             x:DataType="vm:HomePageViewModel"
             x:Class="AvaloniaTest.Views.HomePageView">


  <Panel Name="HomePagePanel" PointerPressed="PagePressed" Background="Transparent">
    
    <Carousel Name="SlideMenu" >
      <Carousel.GestureRecognizers>
        <ScrollGestureRecognizer CanHorizontallyScroll="True" CanVerticallyScroll="True"/>
      </Carousel.GestureRecognizers>
      
      <Canvas
        HorizontalAlignment="Center"
        Width="1024"
        Height="470"
        >
        <!--KOLUMNA I-->
        <!--PIERWSZE OKIENKO {Binding TimeProp.CurrentTime} {Binding TimeProp.CurrentDate} {Binding TimeProp.CurrentDay} -->
        
        <Border  
           Canvas.Top="10" Canvas.Left="10"
           BorderBrush="{DynamicResource BorderColor}"
                        BorderThickness="2"                     
                        BoxShadow="2 2 2 2 Gray"
                        CornerRadius="10"
           >
          <StackPanel         
            Width="243.5"
            Height="146.5"
            VerticalAlignment="Center">
         
            <StackPanel Orientation="Horizontal" >
              <PathIcon Data="{StaticResource ClockRegular}" VerticalAlignment="Center" Margin="10,-6,10,0"></PathIcon>
              <TextBlock Margin="4,-5,0,0" Text="{Binding TimeProp.CurrentDay}" FontSize="22" VerticalAlignment="Center"></TextBlock>
              <Image Source="{Binding WeatherController.CurrentIcon}" Height="52" Width="52" VerticalAlignment="Center" ></Image>
            </StackPanel>           
            <Rectangle Height="2" Width="243.5" Fill="{DynamicResource BorderColor}" Margin="0,-10,0,0"></Rectangle>
            <TextBlock HorizontalAlignment="Center"  FontSize="50" Margin="0,0,0,0" Text="{Binding TimeProp.CurrentTime}"></TextBlock>           
            <TextBlock Margin="0,-9,0,0" HorizontalAlignment="Center" Text="{Binding TimeProp.CurrentDate}" FontSize="15"></TextBlock>
          </StackPanel>
        </Border>
        <!--DRUGIE OKIENKO kolumna I-->


        <!--KOLUMNA II-->
        <Border
          Canvas.Top="10" Canvas.Left="263.5"
          BorderBrush="{DynamicResource BorderColor}"
          BorderThickness="2"
          BoxShadow="2 2 5 2 Gray"
          CornerRadius="10">
            <!--pierwszy panel-->
        <StackPanel       
          Width="243.5"
          Height="196.5"
          VerticalAlignment="Center">
          
          <StackPanel  Orientation="Horizontal">
            <PathIcon Data="{StaticResource HomeRegular}" VerticalAlignment="Top" Margin="10,8,0,8"></PathIcon>
            <PathIcon Data="{StaticResource ArrrowLeft}" Height="10" Margin="-4,14,10,8" VerticalAlignment="Top" ></PathIcon>

            <TextBlock VerticalAlignment="Center"  FontWeight="DemiBold" Margin="20,5,0,0">TEMPERATURA</TextBlock>
          </StackPanel>

          <Rectangle Height="2" Width="243.5" Fill="{DynamicResource BorderColor}" Margin="0,10,0,0"></Rectangle>
          <StackPanel Orientation="Horizontal">
            <Image Source="avares://AvaloniaTest/Assets/Images/thermometer.png" Width="50" Height="50" Margin="0,10,0,0" ></Image>
            <TextBlock Text="{Binding Indoortemperature}" FontWeight="DemiBold" FontSize="35" 
                       TextAlignment="Center"
                       Width="145"                    
                       VerticalAlignment="Center"
                       Margin="5,10,0,0">C</TextBlock>
          </StackPanel>
        
          <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="20" >
            <StackPanel >
              <TextBlock Margin="0,15,0,0" Width="45" TextAlignment="Center">MAX</TextBlock>
              <TextBlock Text="{Binding Indoortodaymaxtemp}"  TextAlignment="Center" Width="45" HorizontalAlignment="Center" Margin="0,10,0,0"></TextBlock>
            </StackPanel>
            <Line StartPoint="0,1" EndPoint="0,60" Margin="0,15,0,0" Stroke="{DynamicResource LightBorderColor}" StrokeThickness="2" StrokeDashArray="3"/>
            <StackPanel Orientation="Vertical">
              <TextBlock Foreground="{DynamicResource FontBlue}" Margin="0,15,0,0" >WCZORAJ</TextBlock>
              <TextBlock Text="{Binding Indoorysterdaytemp}" FontSize="19" Width="55" TextAlignment="Center" HorizontalAlignment="Center" Margin="0,10,0,0"></TextBlock>
              <PathIcon   Data="{Binding Indoortemperaturechangeicon}" Foreground="{DynamicResource LightBorderColor}"  VerticalAlignment="Center" Margin="-55,-23,5,0"></PathIcon>
            </StackPanel>
           
            <Line StartPoint="0,1" EndPoint="0,60" Margin="0,15,0,0" Stroke="{DynamicResource LightBorderColor}" StrokeThickness="2" StrokeDashArray="3"/>
            <StackPanel>
              <TextBlock Margin="-10,15,0,0" Width="45"  TextAlignment="Center"                           
                         >MIN</TextBlock>
              <TextBlock Text="{Binding Indoortodaymintemp}" HorizontalAlignment="Center" Width="45" TextAlignment="Center" Margin="-10,10,0,0"></TextBlock>            
            </StackPanel>

          </StackPanel>
        </StackPanel>         
        </Border>

        <!--drugi panel - druga kolumna-->
        <Border
      Canvas.Top="216.5" Canvas.Left="263.5"
