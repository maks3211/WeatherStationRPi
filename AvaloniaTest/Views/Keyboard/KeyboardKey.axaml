<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="AvaloniaTest.Views.KeyboardKey">
  <Button x:Name="MainButton" Click="OnButtonClick" Width="60" Height="60" >
      <StackPanel>
        <TextBlock x:Name="PrimaryTextBlock"   Foreground="Green"/>
        <TextBlock x:Name="SecondaryTextBlock" Foreground="Red"/>
      </StackPanel>
    </Button>
</UserControl>
