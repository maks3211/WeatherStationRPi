<UserControl xmlns="https://github.com/avaloniaui"
			       xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:AvaloniaTest.ViewModels"
             xmlns:lvc="using:LiveChartsCore.SkiaSharpView.Avalonia"
             mc:Ignorable="d" d:DesignWidth="1024" d:DesignHeight="470"
						 x:Class="AvaloniaTest.Views.ChartView"
						 x:DataType="viewModels:ChartViewModel">
  <StackPanel>
    <Button Command="{Binding CharButtCommand}">guzik</Button>
    <lvc:CartesianChart
  Width="500"
  Height="500"
  Background="red"
	Series="{Binding Series}"
	Title="{Binding Title}">
    </lvc:CartesianChart>
  </StackPanel>


	<!--<ScottPlot:AvaPlot Name="AvaPlot1"/>-->


</UserControl>
